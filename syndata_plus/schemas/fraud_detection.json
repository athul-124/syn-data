{
  "columns": {
    "transaction_id": {
      "sdtype": "id",
      "regex_format": "TXN[0-9]{8}"
    },
    "customer_id": {
      "sdtype": "id",
      "regex_format": "CUST[0-9]{6}"
    },
    "transaction_amount": {
      "sdtype": "numerical",
      "computer_representation": "Float64",
      "min_value": 1.0,
      "max_value": 10000.0
    },
    "transaction_timestamp": {
      "sdtype": "datetime",
      "datetime_format": "%Y-%m-%d %H:%M:%S"
    },
    "merchant_category": {
      "sdtype": "categorical",
      "categories": [
        "grocery",
        "gas",
        "restaurant",
        "retail",
        "online",
        "entertainment",
        "travel",
        "utilities",
        "healthcare",
        "other"
      ]
    },
    "merchant_id": {
      "sdtype": "id",
      "regex_format": "MERCH[0-9]{6}"
    },
    "payment_method": {
      "sdtype": "categorical",
      "categories": [
        "credit_card",
        "debit_card",
        "digital_wallet",
        "bank_transfer",
        "cryptocurrency"
      ]
    },
    "customer_location": {
      "sdtype": "categorical",
      "categories": [
        "domestic",
        "international"
      ]
    },
    "transaction_velocity": {
      "sdtype": "numerical",
      "computer_representation": "Float64",
      "min_value": 0.0,
      "max_value": 100.0
    },
    "account_age_days": {
      "sdtype": "numerical",
      "computer_representation": "Int64",
      "min_value": 1,
      "max_value": 3650
    },
    "previous_fraud_flags": {
      "sdtype": "numerical",
      "computer_representation": "Int64",
      "min_value": 0,
      "max_value": 10
    },
    "is_fraudulent": {
      "sdtype": "boolean"
    }
  }
}
